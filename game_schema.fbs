namespace Gameplay;

struct PlayerControl {
  up:bool;
  down:bool;
  left:bool;
  right:bool;
}

struct PlayerPosition {
  x:float;
  y:float;
}

table Gameplay {
  player_controls:PlayerControl;
  player_position:PlayerPosition;
  player_id:string;
}

table GameEvent {
    event_type:string;
}

root_type Gameplay;
